
public class UpdatedRunner {

	private static final String Division = null;

	public static void main(String[] args) {
		basicMath(10, 5, Division);
	}

	public static double basicMath(double a, double b, String c) {

		double d = 0;
		if (c.equals("Addition")) {
			return d = a + b;
		} else if (c.equals("Subtraction")) {
			return d = a - b;
		} else if (c.equals("Multiplcation")) {
			return d = a * b;
		} else if (c.equals("Division")) {
			return d = a / b;
		}
		return d;

	}
}