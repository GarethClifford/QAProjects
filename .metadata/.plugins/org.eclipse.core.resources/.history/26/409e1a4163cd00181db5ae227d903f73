package com.qa.Goldilock;

import java.util.ArrayList;
import java.util.List;
import java.util.stream.Collectors;

/**
 * Hello world!
 *
 */
public class Runner {
	public static void main(String[] args) {

		int Number = 1;
		List<Chair> Table = new ArrayList<Chair>();

		Goldilocks goldy = new Goldilocks(100, 80);

		Table.add(new Chair(30, 50));
		Table.add(new Chair(130, 60));
		Table.add(new Chair(90, 60));
		Table.add(new Chair(150, 85));
		Table.add(new Chair(120, 70));
		Table.add(new Chair(200, 200));
		Table.add(new Chair(110, 100));

		System.out.println(Table.stream().filter(s -> goldy.canSit(s)).collect(Collectors.toList()));

	}

}
