package com.qa.Person;

import java.util.ArrayList;
import java.util.stream.Collectors;

public class Runner {

	public static void main(String[] args) {

		Person p1 = new Person("Gareth", "QA Consultant", 23);
		Person p2 = new Person("Joseph", "QA Consultant", 24);
		Person p3 = new Person("Ben", "QA Consultant", 25);
		Person p4 = new Person("Harrison", "QA Consultant", 21);
		Person p5 = new Person("Vinul", "QA Consultant", 23);

		// System.out.println(p1.toString());
		// System.out.println(p2.toString());
		// System.out.println(p3.toString());
		// System.out.println(p4.toString());
		// System.out.println(p5.toString());

		ArrayList<Person> teamMembers = new ArrayList<>();

		teamMembers.add(0, p1);
		teamMembers.add(1, p2);
		teamMembers.add(2, p3);
		teamMembers.add(3, p4);
		teamMembers.add(4, p5);

//		System.out.println(teamMembers);
//		int i;
//		for (i = 0; i < teamMembers.size(); i++) {
//			System.out.println(teamMembers.get(i).name);
//		}
		teamMembers.stream().map(i -> i.name).collect(Collectors.toList()).forEach(System.out::println);

	}

}
