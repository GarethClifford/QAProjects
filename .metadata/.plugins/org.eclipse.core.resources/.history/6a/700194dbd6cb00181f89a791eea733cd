package com.qa.UpdatedCalculator;

public class Runner {
	public static void main(String[] args) {
		Logic log = new Logic();
		
		System.out.println("10 - 5 = " + log.operation(10, 5, subtraction));
		System.out.println("10 + 5 = " + log.operation(10, 5, addition));
		System.out.println("10 * 5 = " + log.operation(10, 5, multiplcation));
		System.out.println("10 / 5 = " + log.operation(10, 5, division));
	}

}
