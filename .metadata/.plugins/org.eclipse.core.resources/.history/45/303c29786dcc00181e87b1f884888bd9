package com.qa.Person;

public class Runner {

	public static void main(String[] args) {

		Person p1 = new Person("Gareth", "QA Consultant", 23);
		Person p2 = new Person("Joseph", "QA Consultant", 24);
		Person p3 = new Person("Ben", "QA Consultant", 25);
		Person p4 = new Person("Harrison", "QA Consultant", 21);
		Person p5 = new Person("Vinul", "QA Consultant", 23);

		System.out.println(p1.name + " " + p1.age + " " + p1.jobTitle);
		System.out.println(p2.name + p2.age + p2.jobTitle);
		System.out.println(p3.name + p3.age + p3.jobTitle);
		System.out.println(p4.name + p4.age + p4.jobTitle);
		System.out.println(p5.name + p5.age + p5.jobTitle);

	}

}
