
public class Runner {

	public static void main(String[] args) {
		System.out.println(Reverse("Vinul"));
	}

	public static String Reverse(String s) {
		int length = s.length();
		String solution = "";
		for (int i = length; i > 0; i--) {
			solution = solution + String.valueOf(s.charAt(i));

		}
		return solution;

	}
}
