package com.qa.ExamResults;

public class Logic {
	static Subject physicsPercentage = a -> (a * 100 / 450);
	static Subject biologyPercentage = a -> (a * 100 / 450);
	static Subject chemistyPercentage = a -> (a * 100 / 450);

	public static double result(double a, Subject subject) {
		return subject.result(a);
	}

	interface Subject {
		double result(double a);
	}
}
