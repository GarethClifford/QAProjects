package com.qa.Garage;

public class Runner {

	public static void main(String[] args) {

		Garage garage = new Garage();
		Vehicle P1 = new Plane(12, "Fly", 4, true);
		Vehicle C1 = new Car(4, "Drive", "1.4L", 5);
		Vehicle M1 = new Motorcycle(2, "Drive", "0.5L", true);
		Plane P2 = new Plane(3, "Fly", 1, false);
		Car C2 = new Car(3, "Drive", "0.5L", 3);
		Motorcycle M2 = new Motorcycle(2, "Drive", "1.3L", false);
		

		garage.addVehicle(P1);
		garage.addVehicle(C1);
		garage.addVehicle(M1);

		garage.addVehicle(P2);
		garage.addVehicle(C2);
		garage.addVehicle(M2);

		System.out.println(P1);
		System.out.println(C1);
		System.out.println(M1);

		System.out.println(P2);
		System.out.println(C2);
		System.out.println(M2);

	}
}
