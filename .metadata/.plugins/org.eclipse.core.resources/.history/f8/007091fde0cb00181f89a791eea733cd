package com.qa.ExamResults;

public class Logic {
	static Subject individualPercentage = a -> (a * 100 / 450);
	// static Subject overallPercentage = (a, b, c) -> (((a + b + c) * 100 / 450));

	public static double result(double a, Subject subject) {
		return subject.result(a);
	}

	interface Subject {
		double result(double a);
	}
}
