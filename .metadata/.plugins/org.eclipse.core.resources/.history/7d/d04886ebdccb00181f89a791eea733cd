package com.qa.ExamResults;

public class Results {
	public static void main(String[] args) {
		Method1(137, 100, 75);
	}

	public static void Method1(double a, double b, double c) {
		System.out.println("Physics: Mark = " + a + ", Percentage" + Logic.result(137, Logic.Percentage) + "%");
		System.out.println("Biology: Mark = " + b + ", Percentage" + Logic.result(100, Logic.Percentage) + "%");
		System.out.println("Chemisty: Mark = " + c + ", Percentage" + Logic.result(75, Logic.Percentage) + "%");
	}

}
