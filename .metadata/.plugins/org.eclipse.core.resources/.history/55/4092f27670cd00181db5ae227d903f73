
public class Calculator {

	public static void calculate(double x, double y, double z) {
		if (x * y == z) {
			System.out.println(Math.round(x) + "*" + Math.round(y) + "=" + Math.round(z));
		}
		if (x + y == z) {
			System.out.println(Math.round(x) + "+" + Math.round(y) + "=" + Math.round(z));
		}
		if (x - y == z) {
			System.out.println(Math.round(x) + "-" + Math.round(y) + "=" + Math.round(z));
		}
		if (x / y == z) {
			System.out.println(Math.round(x) + "/" + Math.round(y) + "=" + Math.round(z));
		}

	}

	public static void calculate(int x, int y) {
		if (x == y) {
			System.out.println(Integer.toString(x) + "=" + Integer.toString(y));

		}

	}

	public static void calculate(double x, double y, double z, double w) {

		System.out.println("This way madness lies");
	}

	public static void calculator(int x, int y, int z) {
		calculate(x, y, z);
		calculate(x, z, y);
		calculate(y, z, x);
		calculate(y, x, z);
		calculate(z, x, y);
		calculate(z, y, x);
		calculate(x, y);
		calculate(y, z);
		calculate(z, x);

	}

	public static void calculafour(int x, int y, int z) {
		calculate(x, y, z);
		calculate(x, z, y);
		calculate(y, z, x);
		calculate(y, x, z);
		calculate(z, x, y);
		calculate(z, y, x);

	}

	public static void calculator(int w, int x, int y, int z) {
		calculate(x, y, z, w);
		calculafour(x, y, z);
		calculafour(y, z, w);
		calculafour(z, w, x);
		calculafour(w, x, y);
		calculate(x, y);
		calculate(x, z);
		calculate(x, w);
		calculate(y, z);
		calculate(y, w);
		calculate(z, w);
	}

}
