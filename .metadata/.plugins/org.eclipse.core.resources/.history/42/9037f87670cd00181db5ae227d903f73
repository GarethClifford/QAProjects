
public class Calculator {

	public static void calculate(double x, double y, double z) {
		if (x * y == z) {
			System.out.println(Math.round(x) + "*" + Math.round(y) + "=" + Math.round(z));
		}
		if (x + y == z) {
			System.out.println(Math.round(x) + "+" + Math.round(y) + "=" + Math.round(z));
		}
		if (x - y == z) {
			System.out.println(Math.round(x) + "-" + Math.round(y) + "=" + Math.round(z));
		}
		if (x / y == z) {
			System.out.println(Math.round(x) + "/" + Math.round(y) + "=" + Math.round(z));
		}

	}

	public static void calculate(int x, int y) {
		if (x == y) {
			System.out.println(Integer.toString(x) + "=" + Integer.toString(y));

		}

	}

	public static void calculat(double x, double y, double z, double w) {

		if (x * y == z * w) {
			System.out.println(Math.round(x) + "*" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x * y == z + w) {
			System.out.println(Math.round(x) + "*" + Math.round(y) + "=" + Math.round(z) + "+" + Math.round(w));
		}
		if (x * y == z / w) {
			System.out.println(Math.round(x) + "*" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x * y == z - w) {
			System.out.println(Math.round(x) + "*" + Math.round(y) + "=" + Math.round(z) + "-" + Math.round(w));
		}
		if (x + y == z * w) {
			System.out.println(Math.round(x) + "+" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x + y == z + w) {
			System.out.println(Math.round(x) + "+" + Math.round(y) + "=" + Math.round(z) + "+" + Math.round(w));
		}
		if (x + y == z / w) {
			System.out.println(Math.round(x) + "+" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x + y == z - w) {
			System.out.println(Math.round(x) + "+" + Math.round(y) + "=" + Math.round(z) + "-" + Math.round(w));
		}
		if (x / y == z * w) {
			System.out.println(Math.round(x) + "/" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x / y == z + w) {
			System.out.println(Math.round(x) + "/" + Math.round(y) + "=" + Math.round(z) + "+" + Math.round(w));
		}
		if (x / y == z / w) {
			System.out.println(Math.round(x) + "/" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x / y == z - w) {
			System.out.println(Math.round(x) + "/" + Math.round(y) + "=" + Math.round(z) + "-" + Math.round(w));
		}
		if (x - y == z * w) {
			System.out.println(Math.round(x) + "-" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x - y == z + w) {
			System.out.println(Math.round(x) + "-" + Math.round(y) + "=" + Math.round(z) + "+" + Math.round(w));
		}
		if (x - y == z / w) {
			System.out.println(Math.round(x) + "-" + Math.round(y) + "=" + Math.round(z) + "*" + Math.round(w));
		}
		if (x - y == z - w) {
			System.out.println(Math.round(x) + "-" + Math.round(y) + "=" + Math.round(z) + "-" + Math.round(w));
		}
		

	}

	public static void calculate(int a, int b, int c, int d) {
		calculat(a,b,c,d); 
		calculat(a,b,d,c);
		calculat(a,c,b,d); 
		calculat(a,c,d,b); 
		calculat(a,d,b,c); 
		calculat(a,d,c,b);
		calculat(b,a,c,d); 
		calculat(b,a,d,c); 
		calculat(b,c,a,d); 
		calculat(b,c,d,a); 
		calculat(b,d,a,c); 
		calculat(b,d,c,a); 
		calculat(c,a,b,d); 
		calculat(c,a,d,b); 
		calculat(c,b,a,d); 
		calculat(c,b,d,a);
		calculat(c,d,a,b);
		calculat(c,d,b,a); 
		calculat(d,a,b,c); 
		calculat(d,a,c,b); 
		calculat(d,b,a,c); 
		calculat(d,b,c,a); 
		calculat(d,c,a,b); 
		calculat(d,c,b,a);

	}
	
	
	public static void calculator(int x, int y, int z) {
		calculate(x, y, z);
		calculate(x, z, y);
		calculate(y, z, x);
		calculate(y, x, z);
		calculate(z, x, y);
		calculate(z, y, x);
		calculate(x, y);
		calculate(y, z);
		calculate(z, x);

	}

	public static void calculafour(int x, int y, int z) {
		calculate(x, y, z);
		calculate(x, z, y);
		calculate(y, z, x);
		calculate(y, x, z);
		calculate(z, x, y);
		calculate(z, y, x);

	}

	public static void calculator(int w, int x, int y, int z) {
		calculate(x, y, z, w);
		calculafour(x, y, z);
		calculafour(y, z, w);
		calculafour(z, w, x);
		calculafour(w, x, y);
		calculate(x, y);
		calculate(x, z);
		calculate(x, w);
		calculate(y, z);
		calculate(y, w);
		calculate(z, w);
	}

}
