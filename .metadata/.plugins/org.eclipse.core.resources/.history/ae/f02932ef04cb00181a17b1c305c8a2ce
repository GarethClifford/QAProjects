
public class UpdatedRunner {

	public static void main(String[] args) {
		basicMath(10, 5, "Add");
	}

	public static double basicMath(double a, double b, String c) {

		double d = 0;
		if (c.equals("Add")) {
			d = a + b;
		} else if (c.equals("Take")) {
			d = a - b;
		} else if (c.equals("Times")) {
			d = a * b;
		} else if (c.equals("Divide")) {
			d = a / b;
		}
		return d;

	}
}